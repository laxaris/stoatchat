# OAuth2 Proxy Configuration
# Copy this file to .env.sso and fill in the values

# OAuth2 Client Secret (get from Keycloak)
OAUTH2_CLIENT_SECRET=your-client-secret-here

# OAuth2 Cookie Secret
# MUST be exactly 16, 24, or 32 bytes (for AES cipher)
# Generate with: openssl rand -hex 16  (this gives 32 hex chars = 16 bytes)
# Do NOT use base64 output directly, it will be too long!
OAUTH2_COOKIE_SECRET=changeme1234567890123456789012

# Keycloak Configuration
KEYCLOAK_REALM=stoatchat
KEYCLOAK_URL=http://localhost:8080

# Stoatchat Configuration
STOATCHAT_API_URL=http://localhost:14702
STOATCHAT_WS_URL=ws://localhost:14703
